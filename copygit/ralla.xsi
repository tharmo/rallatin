   xse:gawk in="../finnwn/lists/fiwn-semrels-extra.tsv" : '''
    BEGIN { FS="\t";print "<ul>"}
     function list(st)    {    res=""; split(st,ss,"|"); 
      for (s in ss) {  
      gsub(/^[[:space:]]+|[[:space:]]+$/,"",ss[s]);
      if (match(ss[s],/[ -_><]/)<1)
       res=res "," ss[s];
            }
           return res
      }
     function short(st)    { 
        res=""; 
       split(st,ss," "); 
       return substr(ss[1],1,3) substr(ss[2],1,3) 
              }
    { res1=list($2);
     res2=list($5);  
     if (short($3)!="ATT") 
     print res1  res2 >"finwn.semrel"
     #if (NR>10) exit
      }
   ''' 
